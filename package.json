{
  "name": "unity-ccd-locker",
  "version": "1.0.1",
  "description": "A git-based locking mechanism for Unity CCD bucket operations.",
  "repository": {
    "type": "git",
    "url": "https://github.com/starikcetin/unity-ccd-locker.git"
  },
  "author": {
    "name": "starikcetin",
    "email": "cetinsamedtarik@gmail.com",
    "url": "https://github.com/starikcetin"
  },
  "license": "MIT",
  "keywords": [
    "unity",
    "ccd",
    "ucd",
    "addressable",
    "asset",
    "bundle",
    "lock",
    "git"
  ],
  "main": "dist/index.js",
  "bin": {
    "unity-ccd-locker": "dist/index.js",
    "ucl": "dist/index.js"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.json",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "generate": "ts-node ./scripts/generate.ts",
    "compile": "tsc",
    "build": "run-s clean generate compile",
    "prestartJs": "run-s build",
    "startJs": "node ./dist/index.js",
    "prestartTs": "run-s generate",
    "startTs": "ts-node ./src/index.ts",
    "lint": "eslint --fix --quiet src/ scripts/",
    "format": "prettier --loglevel silent --write src/ scripts/",
    "prepublishOnly": "run-s build",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "node-unique-machine-id": "^1.1.0",
    "semver": "^7.5.3",
    "simple-git": "^3.19.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.3",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.3",
    "@types/extract-zip": "^2.0.1",
    "@types/node": "^20.3.0",
    "@types/semver": "^7.5.0",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "typescript-eslint-parser": "^22.0.0"
  }
}
